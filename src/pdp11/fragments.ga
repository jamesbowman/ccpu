---------------------------- 507 ----------------------------
  @p a!
    EAST
: main
  @ dup @p 
    WEST
  b! !b dup @p 
    NORTH
  b! !b dup @p 
    SOUTH
  b! !b @p @p
    3
    NORTH_SOUTH_WEST
  b! push

  @b ! unext
 jump main

define(`searchnode', `
\ fetch instruction from b
\ search table
\   send 4 words if match
\ do nothing if not found
: searcher
  @b @p a!
    table
: checkloop
  dup @+ if searcher
  and @+ or
  if match
  drop a @p +
    4
  a! jump checkloop
: match
  @p push
    3
  @+ !b unext
  jump searcher

: table

')

