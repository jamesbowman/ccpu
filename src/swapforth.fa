define(GO, `
  @p call `GO'
    $1
')
define(TRACE, `
  a @p @p
    $1
    EAST
  a! ! a!
')

BLOCK one
  a!
  TRACE(11111)
  jump 0
: cold
  a push
  @p a! !
    EAST
  pop a!
  @+
  jump cold

BLOCK two
    a! jump 0
: cold
  TRACE(22222)
  GO(one)
  jump NORTH

BLOCK three
  TRACE(33333)
  @p . +                \ 1+
    1
  push @ pop            \ SWAP
  !
  GO(two)

BLOCK zero
  @p
    101                 \ 101
  dup call -!           \ DUP
  @p call LIT           \ 202
    202
  @+ . +                \ +
  @p call LIT           \ 99999
    99999
  GO(three)
